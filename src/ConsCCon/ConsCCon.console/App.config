<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <appSettings>
    <!-- Dados do certificado do cliente -->
    <add key="CNPJCliente" value="00000000000000" />
    <add key="UFCliente" value="PR" />
    
    <add key="PastaEnvioUninfe" value="c:\temp\ConsCCon" />
    <add key="PastaRetornoUninfe" value="c:\temp\ConsCCon" />
    <add key="PastaArquivoCSV" value="c:\temp" />
    
    <!-- Posição inicia com 1 -->
    <add key="ColunaCnpj" value="1" />
    <add key="ColunaUF" value="3"/>
    <add key="TagsRetornoXml" value="IE;CNPJ;xNome;xLgr;nro;xCpl;xBairro;cMun;xMun;CEP;cPais;xPais;fone;email;cSit;xSit;dhSit;cNAE;cnae;CRT;indCredNFe;CSOSN;cStat;xMotivo"/>
    
    <!-- 
			Padrões aceitos: DDMMYYYY, YYYYMMDD, YYYYMM, MMYYYY 
      Deve incluir a extensão.
	  -->
    <add key="NomeArquivoCSV" value="arq_MMYYYY.csv"/>

    <!-- 
			Tempo em segundos para esperar para repetir leitura da pasta de retorno do UNINFE 
			Mínimo: 30, máximo: 600 (10 minutos)
    -->
    <add key="SegundosEsperaRetorno" value="30"/>
    
    <!--
			Número de repetições de leitura do retorno
      Mínimo: 1, máximo: 10
    -->
    <add key="RepeticoesRetorno" value="10" />

    <add key="PastaArquivosLidos" value="c:\temp\lidos" />
    <add key="PadraoArquivoRet" value="*-ret-cons-cad.xml"/>
  </appSettings>
</configuration>